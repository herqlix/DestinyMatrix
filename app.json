{
  "expo": {
    "name": "DestinyMatrix",
    "slug": "DestinyMatrix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/matrix_icon.png",
    "scheme": "destinymatrix",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFB6C1",
        "foregroundImage": "./assets/images/matrix_icon.png"
      
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/matrix_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
            "backgroundColor": "#FFB6C1",
          "dark": {
        "backgroundColor": "#FFB6C1" 
      }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
